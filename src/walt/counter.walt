let counter: i32 = 0;

export function decrement(): i32 {
  counter -= 1;
  return counter;
}

export function increment(): i32 {
  counter += 1;
  return counter;
}

export function count(v:i32): i32 {
  let j: i32 = v;
  while (j > 0) {
    j -= 1;
    let i: i32 = v;
    while (i > 0) {
        i -= 1;
    }
  }
  return 0;
}
